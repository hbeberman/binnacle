{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-redeclare": "error",
    "no-undef": "error"
  },
  "ignorePatterns": [
    "node_modules/",
    "target/",
    "web/js/**/*.test.js"
  ]
}
